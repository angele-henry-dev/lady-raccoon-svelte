		{
			"src": "/tools/contrast-checker/",
			"status": 308,
			"headers": {
				"Location": "/tools/contrast-checker"
			}
		},
		{
			"src": "/blog/2025-04-22-presentation",
			"dest": "/blog/2025-04-22-presentation/"
		},
		{
			"src": "/blog/2025-04-22-presentation/",
			"status": 308,
			"headers": {
				"Location": "/blog/2025-04-22-presentation"
			}
		},
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/.*",
			"dest": "/fn"
		}
	],
	"overrides": {
		"index.html": {
			"path": ""
		},
		"blog.html": {
			"path": "blog"
		},
		"legal-notice.html": {
			"path": "legal-notice"
		},
		"privacy-policy.html": {
			"path": "privacy-policy"
		},
		"resources.html": {
			"path": "resources"
		},
		"sitemap.html": {
			"path": "sitemap"
		},
		"tools.html": {
			"path": "tools"
		},
		"tools/contrast-checker.html": {
			"path": "tools/contrast-checker"
		},
		"blog/2025-04-22-presentation.html": {
			"path": "blog/2025-04-22-presentation"
		}
	},
	"images": {
		"sizes": [
			640,
			828,
			1200,
			1920
		],
		"formats": [
			"image/avif",
			"image/webp"
		],
		"minimumCacheTTL": 300,
		"domains": [
			"lady-raccoon-svelte.vercel.app"
		]
	}
}