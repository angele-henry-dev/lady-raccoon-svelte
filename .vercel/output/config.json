{
	"version": 3,
	"routes": [
		{
			"src": "/blog",
			"dest": "/blog/"
		},
		{
			"src": "/blog/",
			"status": 308,
			"headers": {
				"Location": "/blog"
			}
		},
		{
			"src": "/legal-notice",
			"dest": "/legal-notice/"
		},
		{
			"src": "/legal-notice/",
			"status": 308,
			"headers": {
				"Location": "/legal-notice"
			}
		},
		{
			"src": "/privacy-policy",
			"dest": "/privacy-policy/"
		},
		{
			"src": "/privacy-policy/",
			"status": 308,
			"headers": {
				"Location": "/privacy-policy"
			}
		},
		{
			"src": "/resources",
			"dest": "/resources/"
		},
		{
			"src": "/resources/",
			"status": 308,
			"headers": {
				"Location": "/resources"
			}
		},
		{
			"src": "/sitemap",
			"dest": "/sitemap/"
		},
		{
			"src": "/sitemap/",
			"status": 308,
			"headers": {
				"Location": "/sitemap"
			}
		},
		{
			"src": "/tools",
			"dest": "/tools/"
		},
		{
			"src": "/tools/",
			"status": 308,
			"headers": {
				"Location": "/tools"
			}
		},
		{
			"src": "/tools/contrast-checker",
			"dest": "/tools/contrast-checker/"
